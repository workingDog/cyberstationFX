            <Insets bottom="12.0" left="4.0" right="4.0" top="12.0" />
         </GridPane.margin>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </JFXTextArea>
      <JFXTextField fx:id="urlField" promptText="url" GridPane.rowIndex="2">
